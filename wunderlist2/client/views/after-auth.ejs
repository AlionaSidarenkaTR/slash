<!doctype html>
<html >
<head>
	<title>Node Authentication</title>
	<script type="text/javascript">
	    if (window.opener) {
	        window.opener.focus();
	        var state = '<%= state %>';
	        var user = <%- JSON.stringify(user) %>;
	        if (window.opener.app &&
	            window.opener.app.authState) {
	            window.opener.app.authState(state, user);
	        }
	    }
	    window.close();
	</script>
</head>
<body>
</body>
</html>